Public Sub Form_Close()

  Dim respuesta As Integer

  respuesta = Message.Question("¿Advertencia: Deseas Cerrar G Radio?", "Cerrar", "Cancelar")
  If respuesta = 2 Then
    Stop Event
  Else
    Shell "~/.gradio/bin/detener.sh"
  Endif
  $bStop = False

End

