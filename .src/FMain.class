Public Sub grupoBotones_MouseDown()

  BotonPulsado = Last
  colorfondo = Last.Background

  Last.Background = Color.red
  Last.Refresh()
  Wait 0.01

  Processtmp = Shell "xterm -geometry 80x3 -title EFECTOS -e mplayer -playlist " & botonPulsado.tag
  '  Processtmp = Exec ["mplayer", "-playlist", botonPulsado.tag]

  While Processtmp.State = Process.Running
    Wait 0.01
    PrepareDraw()
  Wend
  BotonPulsado.Background = colorfondo

End

